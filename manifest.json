{
  "author": "igorlogius",
  "browser_action": {
    "default_popup": "background.html",
    "default_title": "Save Website as Image"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Alt+S"
      }
    }
  },
  "description": "Save page in the activ tab as an png image",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "save-website-as-image",
  "permissions": [
    "activeTab",
    "<all_urls>"
  ],
  "version": "1.0.5"
}
