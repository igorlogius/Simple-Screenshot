{
  "author": "igorlogius",
  "browser_action": {
    "default_popup": "background.html",
    "theme_icons": [
      {
        "light": "dark-256.png",
        "dark": "light-256.png",
        "size": 256
      },
      {
        "light": "dark-128.png",
        "dark": "light-128.png",
        "size": 128
      },
      {
        "light": "dark-64.png",
        "dark": "light-64.png",
        "size": 64
      },
      {
        "light": "dark-32.png",
        "dark": "light-32.png",
        "size": 32
      },
      {
        "light": "dark-16.png",
        "dark": "light-16.png",
        "size": 16
      }
    ]
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Alt+S"
      }
    }
  },
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "Save Website as Image",
  "permissions": [
    "activeTab",
    "<all_urls>"
  ],
  "version": "1.2.10"
}
